{
  "printWidth": 120,
  "singleQuote": true,
  "tabWidth": 4,
  "overrides": [
      {
          "files": "*.json",
          "options": {
              "tabWidth": 2
          }
      },

      {
          "files": "*.md",
          "options": {
              "printWidth": 80,
              "tabWidth": 2,
              "proseWrap": "always"
          }
      }
  ]
}
